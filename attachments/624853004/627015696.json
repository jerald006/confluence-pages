							"})\r",
							"\r",
							"/****************************  Functions  ****************************/\r",
							"function log(supplier, supplierCode, recordLocator, token) {\r",
							"    const moment = require('moment')\r",
							"\r",
							"    let idWeb = token.substring(17)\r",
							"    let env = token.substring(16, 17)\r",
							"\r",
							"    switch (env) {\r",
							"        case 'D':\r",
							"            env = \"Desa\"\r",
							"            break\r",
							"        case 'P':\r",
							"            env = \"Prod\"\r",
							"            break;\r",
							"        default:\r",
							"            env = \"Check\"\r",
							"    }\r",
							"\r",
							"    let PNRsLog = pm.collectionVariables.get(\"PNRs-Log\")\r",
							"\r",
							"    if (PNRsLog === undefined) PNRsLog = \"\"\r",
							"    else if (PNRsLog !== '' || PNRsLog !== undefined) PNRsLog += \" ** \"\r",
							"    \r",
							"    PNRsLog += `${moment().format((\"YYYY-MM-DD - HH:mm:ss\"))} | PNR: ${recordLocator} | Source: ${supplier} - ${supplierCode} | Environment: ${env} | IdWeb: ${idWeb}`\r",
							"\r",
							"    pm.collectionVariables.set(\"PNRs-Log\", PNRsLog)\r",
							"}"
						],
