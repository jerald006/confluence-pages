							"        opt.Segments.forEach(seg => {\r",
							"            console.log(`\\t\\t${indexLeg}-${indexOpt} -- ${seg.Airline} ${seg.FlightNumber}  ${seg.Departure.AirportCode} ${seg.Arrival.AirportCode} ${seg.Departure.Date} ${seg.Departure.Time}`)\r",
							"        })\r",
